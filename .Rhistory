regtry <- function(y, x){
b.one <- (sum(y*x)-(sum(x)*sum(y)/length(x)))/(sum(x^2)-(sum(x)^2/length(x)))
b.zero <- mean(y) - b.one*mean(x)
result <- data.frame("Intercept" = b.zero, "b1" = b.one)
return(result)
}
